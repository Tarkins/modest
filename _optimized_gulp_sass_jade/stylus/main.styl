@import '_vars.styl'
@import 'mixins.styl'
@import 'fonts.styl'
@import 'optimize.styl'
@import '../jade/**/*'

//Load Libs SASS
@import 'animate.css'

*::-webkit-input-placeholder
	color: #666
	opacity: 1

*:-moz-placeholder
	color: #666
	opacity: 1

*::-moz-placeholder
	color: #666
	opacity: 1

*:-ms-input-placeholder
	color: #666
	opacity: 1

body input:focus:required:invalid,
body textarea:focus:required:invalid
	color: #666
body input:required:valid,
body textarea:required:valid
	color: #666

